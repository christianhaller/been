{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-deno@0.7.10"
    }
  },
  "rewrites": [
    { "source": "/api/highscore/alltime", "destination": "/api/highscore?alltime" }
  ],
  "build": {
    "env": {
      "DENO_VERSION": "1.9.2"
    }
  },
  "github": {
    "enabled": true
  },
  "public": true
}
