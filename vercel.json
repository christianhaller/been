{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-deno@0.7.12"
    }
  },
  "rewrites": [
    {
      "source": "/api/highscore/alltime",
      "destination": "/api/highscore?alltime"
    }
  ],
  "build": {
    "env": {
      "DENO_VERSION": "1.10.2"
    }
  },
  "github": {
    "enabled": true
  },
  "public": true
}