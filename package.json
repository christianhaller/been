{
  "name": "download-your-travelmap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "badges": "ts-node --transpile-only coverage",
    "build:css": "postcss src/frontend/styles/styles.css > public/styles.min.css",
    "build:html": "html-minifier --collapse-whitespace --input-dir src/frontend/templates --output-dir public",
    "build:js": "rollup -c ./rollup.config.js",
    "build": "NODE_ENV=production node ./build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:e2e": "jest -c e2e/jest.config.js",
    "test:frontend": "jest",
    "test:backend": "deno test --allow-read --allow-net --log-level info --allow-env src/backend",
    "fmt": "deno fmt src/backend api && prettier 'src/frontend/**/*' '!**/*.snap' --write"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "country-emoji": "^1.5.4",
    "geojson": "^0.5.0",
    "json2csv": "^5.0.3",
    "jszip": "^3.5.0",
    "loadjs": "^4.2.0",
    "tokml": "^0.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/google.visualization": "0.0.54",
    "@types/json2csv": "^5.0.1",
    "@types/jszip": "^3.4.1",
    "autoprefixer": "^10.0.1",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "jest": "^26.6.1",
    "jest-puppeteer": "^4.4.0",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^13.0.0",
    "prettier": "^2.1.2",
    "puppeteer": "^5.3.1",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^1.9.6",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-ch:christianhaller/download-your-travelmap.git"
  },
  "keywords": [],
  "author": "Christian Haller",
  "license": "ISC"
}
