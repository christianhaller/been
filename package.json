{
  "name": "download-your-travelmap",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "badges": "ts-node --transpile-only coverage",
    "build": "parcel build src/frontend/templates/index.pug --no-cache && cpy src/frontend/robots.txt public",
    "test": "npm run test:backend && npm run test:frontend",
    "test:e2e": "jest -c e2e/jest.config.js",
    "test:frontend": "jest",
    "test:backend": "deno test --reload --unstable --coverage --allow-read --allow-net --log-level info --allow-env src/backend",
    "fmt": "deno fmt src/backend api && prettier 'src/frontend/**/*' '!**/*.snap' --write",
    "deps:frontend": "ncu",
    "deps:backend": "npm run dmm -- check",
    "deps": "npm run deps:backend && npm run deps:frontend",
    "dmm": "deno run --allow-write --allow-net --allow-read https://deno.land/x/dmm@v1.1.5/mod.ts",
    "upgrade:backend": "npm run dmm -- update",
    "upgrade:frontend": "ncu -u",
    "upgrade": "npm run upgrade:backend && npm run upgrade:frontend"
  },
  "app": "public/index.html",
  "targets": {
    "app": {
      "publicUrl": "./"
    }
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "country-emoji": "^1.5.4",
    "geojson": "^0.5.0",
    "json2csv": "^5.0.5",
    "jszip": "^3.5.0",
    "load-js": "^3.0.3",
    "tokml": "^0.4.0"
  },
  "devDependencies": {
    "@types/google.visualization": "0.0.58",
    "@types/json2csv": "^5.0.1",
    "@types/jszip": "^3.4.1",
    "autoprefixer": "^10.0.4",
    "cpy-cli": "^3.1.1",
    "cssnano": "^4.1.10",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "npm-check-updates": "^10.2.3",
    "parcel": "^2.0.0-nightly.492",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.1.14",
    "postcss-import": "^13.0.0",
    "postcss-modules": "^4.0.0",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "puppeteer": "^5.5.0",
    "tailwindcss": "^2.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-ch:christianhaller/download-your-travelmap.git"
  },
  "keywords": [],
  "author": "Christian Haller",
  "license": "ISC"
}
